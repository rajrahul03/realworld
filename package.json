  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prettier:write": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "lint:check": "npx eslint apps/**/*.ts",
    "lint:fix": "npx eslint --fix apps/**/*.ts",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "storybook": "nx run storybook:storybook",
    "build-storybook": "nx run storybook:build-storybook",
    "chromatic": "npx chromatic",
    "build:prod": "nx build --prod" // Added build:prod script
  },
  "prisma": {
    "seed": "tsx ./apps/api/src/prisma/seed.ts",
    "schema": "./apps/api/src/prisma/schema.prisma"
  },
